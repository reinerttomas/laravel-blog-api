{
    "preset": "laravel",
    "rules": {
        "declare_strict_types": true,
        "blank_line_between_import_groups": true,
        "concat_space": {
            "spacing": "one"
        },
        "class_attributes_separation": {
            "elements": {
                "method": "one"
            }
        },
        "new_with_braces": {
            "named_class": false,
            "anonymous_class": false
        },
        "ordered_imports": {
            "sort_algorithm": "alpha",
            "imports_order": [
                "const",
                "class",
                "function"
            ]
        },
        "phpdoc_align": false
    }
}
